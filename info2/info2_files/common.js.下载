function showWait(result) {
    var times = 1500;
    if(result === false) {
        $('.wait').css('display','none')
        times = 200;
    }
    var html = '<div class="wait"><ul><li style="width: 25px;height: 25px;background: #fff;"></li><li></li><li></li><li></li></ul></div>';
    $('body').append(html);

    //进入首页缓冲js
    setTimeout(function() {
        $('.wait').css('display','none')
    },times)

    var len = $('.wait ul li').length;
    var index = 0;
    setInterval(function() {
        $('.wait ul li').css({'width':'20px','height':'20px','background':'#eee'})
        if(index > len){
            index = 0;
        }
        $('.wait ul li').eq(index).css({'width':'25px','height':'25px','background':'#fff'})
        index++;
    },200)
}

