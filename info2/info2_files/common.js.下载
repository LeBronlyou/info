function parse_request(params) {
    var cur_url = window.location.href;
    var use_url = params.url;
/*
    request_ajax(params);
    return ;
    if(cur_url.substring(0,cur_url.indexOf('.')) == use_url.substring(0,use_url.indexOf('.'))){
        request_ajax(params);
    }else{
        alert(2);
        request_cross_ajax(params);
    }
*/
    request_ajax(params);
}

//跨域
function request_cross_ajax(params){
    $.ajax({
        //脚本地址
        url:params.url,
        type:'get',
        data:params.data ? params.data : {},
        //async : false,
        async : params.async===false ? false : true,
        dataType:'jsonp',
        jsonpCallback : 'callback',
        success:function(data){
            params.callback(data);
        }
    });
}
//原生
function request_ajax(params){
    $.ajax({
        //脚本地址
        url:params.url,
        type:params.type ? params.type : 'get',
        data:params.data,
        async : params.async===false ? false : true,
        dataType:'json',
        success:function(data){
            params.callback(data);
        }
    })
}

function handleStatus(params,func) {
    if(params.status == 101 ){
        func(params);
    }else if(params.status == 201) {
        alert('数据库错误');
    }else if(params.status == 301) {
        alert('参数错误');
    }else if(params.status == 401) {
        window.location.href = params.login_url;
    }
}

//解决快速连续点击事件
var lastClick;
function lockClick() {
    var nowClick = new Date();
    if (lastClick == null) {
        lastClick = nowClick;
        return true;
    } else {
        if (Math.round((nowClick.getTime() - lastClick.getTime())) > 500) {
            lastClick = nowClick;
            return true;
        }
        else {
            lastClick = nowClick;
            return false
        }
    }
}


//公共提示弹层
//普通弹窗提示
function showalert (text, type) {
    var flg_class = (type == 1) ? 'suc' : ((type == 2) ? 'fail' : ''),
        html = '<div class="alert '+ flg_class +'"><div class="alert_bg"></div><div class="alert_text">' + text + '</div></div>';
    $ ("body").append (html);
    if(type == 4){
        $ (".alert").hide();
    }else if(type != 3){
        var t = setTimeout (function () {
            $ (".alert").fadeOut (1000);
        }, 1000);
        var s = setTimeout (function () {
            $ (".alert").remove ();
        }, 2000);
    }
}


